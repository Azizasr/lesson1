1
console.log("Все числа от 1 до 20:");
for (let i = 1; i <= 20; i++) {
  console.log(i);
}
2
console.log("\nЧётные числа от 1 до 20:");
for (let i = 1; i <= 20; i++) {
  if (i % 2 === 0) {
    console.log(i);
  }
}
3
let sum = 0;
for (let i = 1; i <= 20; i++) {
  if (i % 2 !== 0) {
    sum += i;
  }
}
console.log("\nСумма всех нечётных чисел от 1 до 20: " + sum);
